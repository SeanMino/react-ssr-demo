    "koa-compress": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.0",
    "koa-logger": "^2.0.0",
    "koa-router": "^7.0.0",
    "koa-session": "^3.4.0",
    "koa-static": "^3.0.0",
    "koa-views": "^5.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^3.0.0",
    "superagent": "^3.3.1"
  },
  "devDependencies": {
    "asset-require-hook": "^1.0.2",
    "autoprefixer": "^6.4.1",
    "babel-core": "^6.13.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "chokidar": "^1.7.0",
    "compression": "^1.7.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.4",
    "css-modules-require-hook": "^4.0.5",
    "deep-equal": "^1.0.1",
    "eslint": "^3.12.2",
