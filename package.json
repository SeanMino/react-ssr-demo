{
  "name": "react-front-demo",
  "version": "0.0.1",
  "description": "An universal React isomorphic boilerplate for building server-side render web app",
  "scripts": {
    "dev": "node ./webpack/server.entry.dev.js",
    "build": "npm run clean && webpack --config ./webpack/webpack.client.pro.js --progress",
    "build_server": "webpack --config ./webpack/webpack.server.pro.js --progress",
    "clean": "rimraf ./dist/client/* && rimraf ./dist/server/* ",
    "start": "npm run build && npm run build_server && node ./dist/server/server.js",
    "server": "npm run build && node ./server/app"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "ejs": "^2.5.5",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
